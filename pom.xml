        <!-- https://mvnrepository.com/artifact/org.apache.commons/commons-lang3 -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.7</version>
        </dependency>

        <!-- myBaits分页插件 start-->
        <dependency>
            <groupId>com.github.pagehelper</groupId>
            <artifactId>pagehelper</artifactId>
            <version>5.1.3</version>
        </dependency>
        <dependency>
            <groupId>com.github.pagehelper</groupId>
            <artifactId>pagehelper-spring-boot-starter</artifactId>
            <version>1.2.4</version>
        </dependency>
        <dependency>
            <groupId>com.github.pagehelper</groupId>
            <artifactId>pagehelper-spring-boot-autoconfigure</artifactId>
            <version>1.2.4</version>
        </dependency>
        <!-- myBaits分页插件 end-->
    </dependencies>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.rcloud</groupId>
                <artifactId>spell-common</artifactId>
                <version>0.0.1</version>
            </dependency>
            <dependency>
                <groupId>com.rcloud</groupId>
                <artifactId>spell-mini-program</artifactId>
                <version>0.0.1</version>
            </dependency>

            <dependency>
                <groupId>com.rcloud</groupId>
                <artifactId>spell-entity</artifactId>
                <version>0.0.1</version>
            </dependency>

            <dependency>
                <groupId>com.rcloud</groupId>
                <artifactId>spell-utils</artifactId>
                <version>0.0.1</version>
            </dependency>


            <dependency>
                <groupId>com.rcloud</groupId>
                <artifactId>spell-dao</artifactId>
                <version>0.0.1</version>
            </dependency>

            <dependency>
                <groupId>com.rcloud</groupId>
                <artifactId>spell-web</artifactId>
                <version>0.0.1</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <!--项目配置 start-->
    <profiles>
        <!--线下-->
        <profile>
            <id>dev</id>

            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>

            <properties>
                <profileActive>dev</profileActive>
                <service.address>localhost</service.address>
                <mysql.user>root</mysql.user>
                <mysql.password>102030</mysql.password>
                <redis.password>10203040</redis.password>
                <logging.level>trace</logging.level>
            </properties>
        </profile>

        <!--线上-->
